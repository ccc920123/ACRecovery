<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/bar">
        <shape>
            <corners android:radius="2dp" />
            <gradient android:startColor="#c8c8c8" android:endColor="#e6e6e6" android:angle="270.0" android:centerColor="#e3e3e3" />
        </shape>
    </item>
    <item android:id="@+id/barbg">
        <clip>
            <shape>
                <corners android:radius="2dp" />
                <gradient android:startColor="@color/register_bg" android:endColor="@color/register_bg" android:centerColor="@color/register_bg" />
            </shape>
        </clip>
    </item>
</layer-list>
